@wire_area{

	defs{
		@in_socket{ text outlet
			@pile{
				@text{
					text{${text}}
					color{0xff0000ff}
				}
				@wire_socket{
					id{ws}
					outlet{${outlet}}
					lp{dx{fill} dy{fill}}
				}
			}
		}

		@out_socket{ text outlet
			@pile{
				@text{
					text{${text}}
					color{0xff00ff00}
				}
				@wire_socket{
					id{ws}
					outlet{${outlet}}
					lp{dx{fill} dy{fill}}
				}
			}
		}
	}

	id{wire_area}

	@size_container{
		lp{ dx{fill} dy{fill}}

		@window{
			x{10} y{20}
			lp{
				dx{100} dy{50}
			}

			title{"window 1"}

			@out_socket{
				id{in11}
				outlet{top}
				text{in1}
			}
		}

		@window{
			x{150} y{20}
			lp{
				dx{100} dy{50}
			}

			title{"window 2"}

			@in_socket{
				id{in11}
				outlet{right}
				text{in1}
			}
		}
	}
}
