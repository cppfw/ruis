{
	"package": {
		"name": "libmorda",
		"repo": "deb",
		"subject": "igagis"
	},
	
	"version": {
		"name": "$(version)"
	},

	"files": [
		{
			"includePattern": "../(.[^/]*(\\.deb)$)",
			"uploadPattern": "$1"
		},
		{
			"includePattern": "../(.[^/]*(\\.dsc)$)",
			"uploadPattern": "$1"
		},
		{
			"includePattern": "../(.[^/]*(\\.tar\\.xz)$)",
			"uploadPattern": "$1"
		},
		{
			"includePattern": "../(.[^/]*(\\.tar\\.gz)$)",
			"uploadPattern": "$1"
		}
	],
	"publish": true,
	"override": false
}
